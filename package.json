{
  "name": "@rikkajs/naive-materials",
  "type": "module",
  "version": "0.0.5",
  "private": false,
  "description": "Naive UI Materials",
  "author": "RikkaJS <mikasa33@qq.com> (https://github.com/RikkaJS/)",
  "license": "MIT",
  "homepage": "https://github.com/RikkaJS/naive-materials",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RikkaJS/naive-materials.git"
  },
  "bugs": {
    "url": "https://github.com/RikkaJS/naive-materials/issues",
    "email": "mikasa33@qq.com"
  },
  "keywords": [
    "naive-materials",
    "naive-ui"
  ],
  "main": "dist/naive-materials.umd.cjs",
  "files": [
    "es/*",
    "lib/*"
  ],
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs --port 5174",
    "docs:build": "vitepress build docs --port 5174",
    "docs:preview": "vitepress preview docs --port 5174"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^18.17.0",
    "@unocss/eslint-config": "^0.55.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.47.0",
    "lint-staged": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "~5.1.6",
    "unocss": "^0.55.1",
    "vite": "^4.4.6",
    "vite-plugin-dts": "^3.5.3",
    "vitepress": "1.0.0-rc.4",
    "vue-tsc": "^1.8.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "directories": {
    "doc": "docs"
  }
}
